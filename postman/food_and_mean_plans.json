            }
          },
          "response": []
        },
        {
          "name": "Update Meal Plan",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{auth_token}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Updated Weight Loss Plan\",\n  \"desc\": \"Updated high protein meal plan\",\n  \"isActive\": true\n}"
            },
            "url": {
              "raw": "http://localhost:8081/api/v1/meal-plans/{{meal_plan_id}}",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8081",
              "path": ["meal-plans", "{{meal_plan_id}}"]
            }
          },
          "response": []
        },
        {
          "name": "Delete Meal Plan",
          "request": {
            "method": "DELETE",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{auth_token}}"
              }
            ],
            "url": {
              "raw": "http://localhost:8081/api/v1/meal-plans/{{meal_plan_id}}",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8081",
              "path": ["meal-plans", "{{meal_plan_id}}"]
            }
          },
          "response": []
        },
        {
          "name": "Assign Meal Plan to User",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{auth_token}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"userId\": \"{{user_id}}\"\n}"
            },
            "url": {
              "raw": "http://localhost:8081/api/v1/meal-plans/{{meal_plan_id}}/assign",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8081",
              "path": ["meal-plans", "{{meal_plan_id}}", "assign"]
            }
          },
          "response": []
