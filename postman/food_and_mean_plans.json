            "url": {
              "raw": "http://localhost:8081/api/v1/meal-plans/{{meal_plan_id}}/assign-bulk",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8081",
              "path": ["meal-plans", "{{meal_plan_id}}", "assign-bulk"]
