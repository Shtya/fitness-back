{
  "info": {
    "name": "Fitness App - Foods & Meal Plans API",
    "description": "Complete API for foods, meal plans, and food logging",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Foods",
      "item": [
        {
          "name": "List Foods",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{auth_token}}"
              }
            ],
            "url": {
              "raw": "http://localhost:8081/api/v1/foods?page=1&limit=10&search=chicken",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8081",
              "path": ["foods"],
              "query": [
                {
                  "key": "page",
                  "value": "1"
                },
                {
                  "key": "limit",
                  "value": "10"
                },
                {
                  "key": "search",
                  "value": "chicken"
                }
              ]
            }
          },
          "response": []
        },
        {
          "name": "Create Food",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{auth_token}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Chicken Breast\",\n  \"calories\": 165,\n  \"protein\": 31,\n  \"carbs\": 0,\n  \"fat\": 3.6,\n  \"unit\": \"g\"\n}"
            },
            "url": {
              "raw": "http://localhost:8081/api/v1/foods",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8081",
              "path": ["foods"]
            }
          },
          "response": []
        },
        {
          "name": "Update Food",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{auth_token}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Chicken Breast (Updated)\",\n  \"calories\": 170,\n  \"protein\": 32\n}"
            },
            "url": {
              "raw": "http://localhost:8081/api/v1/foods/{{food_id}}",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8081",
              "path": ["foods", "{{food_id}}"]
            }
          },
          "response": []
        },
        {
          "name": "Delete Food",
          "request": {
            "method": "DELETE",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{auth_token}}"
              }
            ],
            "url": {
              "raw": "http://localhost:8081/api/v1/foods/{{food_id}}",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8081",
              "path": ["foods", "{{food_id}}"]
            }
          },
          "response": []
        },
        {
          "name": "Log Food",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{auth_token}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"foodId\": \"{{food_id}}\",\n  \"date\": \"2024-01-15\",\n  \"mealType\": \"lunch\",\n  \"quantity\": 200,\n  \"notes\": \"Grilled chicken with vegetables\"\n}"
            },
            "url": {
              "raw": "http://localhost:8081/api/v1/foods/log",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8081",
              "path": ["foods", "log"]
            }
          },
          "response": []
        },
        {
          "name": "Get My Food Logs",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{auth_token}}"
              }
            ],
            "url": {
              "raw": "http://localhost:8081/api/v1/foods/logs/my?date=2024-01-15",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8081",
              "path": ["foods", "logs", "my"],
              "query": [
                {
                  "key": "date",
                  "value": "2024-01-15"
                }
              ]
            }
          },
          "response": []
        }
      ]
    },
    {
